<h1>Faculties#show</h1>
<p><b><%= @faculty.name %></b></p>
<table>
	<tr>
		<th>id</th>
		<th>name</th>
		<th>grade</th>
	</tr>
	<% @students.each do |s| %>
		<% if @faculty.id == s.faculty_id %>
			<tr>
				<td><%= s.id %></td>
				<td><%= s.name %></td>
				<td><%= s.grade %></td>
			</tr>
		<% end %>
	<% end %>
</table>
